{
    "name": "PhishNet Extension",
    "version": "1.0.0",
    "description": "Browser Extension of PhishNet",
    "manifest_version": 2,
    "author": "Segmentation Fault",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "activeTab",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": ["malicious_db.txt"],
    "icons": {
        "128": "logo.png" 
    }
}